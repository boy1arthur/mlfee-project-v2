{
  "builds": [
    {
      "src": "blog/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "." }
    }
  ],
  "rewrites": [
    {
      "source": "/blog/:path*",
      "destination": "/blog/:path*"
    }
  ]
}